    "aws-sdk": "^2.1373.0",
    "axios": "1.4.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.1",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.0",
    "dynamoose": "^3.2.0",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "firebase-admin": "^11.8.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jimp": "^0.22.7",
    "jsdom": "^22.0.0",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "nestjs-dynamoose": "^0.5.4",
    "nestjs-paginate": "^4.5.1",
    "nestjs-pino": "^3.2.0",
    "passport": "^0.6.0",
